<script type="text/javascript">

    $( ".MultiLineLimit" ).on( 'change keydown paste input', function () {
        this.value = ( this.value.length <= 150 ? this.value : this.value.substring( 0, 150 ) );
    } );
    var text_max = 150;
    $( '#count_message' ).html( text_max + ' remaining' );

    $( '#Purpose' ).keyup( function () {
        var text_length = $( '#Purpose' ).val().length;
        var text_remaining = text_max - text_length;

        $( '#count_message' ).html( text_remaining + ' remaining' );
    } );

    var jQuery_3_3_1 = $.noConflict( true );

    jQuery_3_3_1( '#EstimatedDistance' ).numeric( {
        maxPreDecimalPlaces: 10,
        maxDecimalPlaces: 2
    } );

    jQuery_3_3_1( '#SubmitTransportRequest' ).on( 'click', function ( event ) {
        event.preventDefault();

        var title = 'Submitting transport request';
        var icon = 'loading';
        var duration = 50000 * 1;

        //validate
        var form = $( "#TransportRequestForm" )

        if ( form[ 0 ].checkValidity() === false ) {
            event.preventDefault()
            event.stopPropagation()
            form.addClass( 'was-validated' );
        } else if ( form[ 0 ].checkValidity() === true ) {
            // Perform ajax submit here...
            jQuery_3_3_1.Toast.showToast( { title: title, duration: duration, icon: icon, image: '' } );

            jQuery.ajax( {
                url: 'ViewTransportRequest.aspx/Save',
                type: "POST",
                data: '{param1:"' + $( '#StartDateTime' ).val() + '", param2:"' + $( '#EndDateTime' ).val() + '", param3:"' + $( '#Purpose' ).val() + '", param4:"' + $( '#DimCode1' ).val() + '", param5:"' + $( '#DimCode2' ).val() + '", param6:"' + $( '#DimCode3' ).val() + '", param7:"' + $( '#DimCode6' ).val() + '", param8:"' + $( '#VehicleClass' ).val() + '", param9:"' + $( '#EstimatedDistance' ).val() + '", param10:"' + $( '#Destination' ).val() + '", param11:"' + $( '#DimCode4' ).val() + '" }',
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function ( response ) {

                    if ( response != null && response.d != null ) {
                        var data = response.d;
                        // alert(typeof (data)); //it comes out to be string
                        //we need to parse it to JSON
                        data = $.parseJSON( data );

                        jQuery_3_3_1.Toast.hideToast();

                        if ( data.Status != "000" ) {

                            bootbox.error( { title: "Approval error", message: data.Message } );
                        } else {

                            bootbox.alert( {
                                message: data.Message,
                                callback: function () {
                                    window.location.href = 'TransportRequests.aspx?status=Pending';
                                }
                            } )
                        }
                    }
                },
                error: function () {
                    // bootbox.error({ title: "System error", message: "An error occured." });
                    jQuery_3_3_1.Toast.hideToast();

                    bootbox.alert( {
                        message: "The advance request was successfully sent for aprroval",
                        callback: function () {
                            window.setTimeout( function () {
                                //alert('redirecting');
                                window.location.href = 'StaffClaims.aspx?status=Pending';
                            }, 1000 );//
                        }
                    } )
                },
                timeout: 10000
            } );
        }
    } );

    jQuery_3_3_1( "#Print" ).on( "click", function ( e ) {
        e.preventDefault();

        const queryString = window.location.search;
        const urlParams = new URLSearchParams( queryString );
        const ApprovalEntryNo = urlParams.get( 'id' )

        jQuery.ajax( {
            url: 'ViewTransportRequest.aspx/PrintRequest',
            type: "POST",
            data: '{param1:"' + ApprovalEntryNo + '"}',
            dataType: "json",
            contentType: "application/json; charset=utf-8",
            success: function ( response ) {

                if ( response != null && response.d != null ) {
                    var data = response.d;
                    // alert(typeof (data)); //it comes out to be string
                    //we need to parse it to JSON
                    data = $.parseJSON( data );

                    bootbox.confirm( {
                        title: "<i class='fa fa-print'></i> Print?",
                        message: "Do you wish to print this transport request",
                        buttons: {
                            confirm: {
                                label: 'Yes',
                                className: 'btn-success'
                            },
                            cancel: {
                                label: 'No',
                                className: 'btn-danger'
                            }
                        },
                        callback: function ( result ) {

                            if ( result == true ) {

                                window.location.href = "uploads/" + data.Status + ".pdf";

                            }
                        }
                    } );

                }
            },
            error: function () {
                hideLoading();
            },
            timeout: 10000
        } );
    } );
    jQuery_3_3_1( '#ExitPage' ).on( 'click', function ( event ) {
        event.preventDefault();
        //return to previous page
        window.history.back();
    } );
    jQuery_3_3_1( '#Approve' ).on( 'click', function ( event ) {
        event.preventDefault();

        const queryString = window.location.search;
        const urlParams = new URLSearchParams( queryString );
        const ApprovalEntryNo = urlParams.get( 'ApprovalEntryNo' )

        bootbox.confirm( {
            title: "<i class='fa fa-check'></i> Approve?",
            message: "Do you wish to approve this approval request?",
            buttons: {
                confirm: {
                    label: 'Yes',
                    className: 'btn-success'
                },
                cancel: {
                    label: 'No',
                    className: 'btn-danger'
                }
            },
            callback: function ( result ) {

                if ( result == true ) {

                    jQuery.ajax( {
                        url: 'ViewTransportRequest.aspx/ApproveApprovalRequest',
                        type: "POST",
                        data: '{param1:"' + ApprovalEntryNo + '"}',
                        dataType: "json",
                        contentType: "application/json; charset=utf-8",
                        success: function ( response ) {

                            if ( response != null && response.d != null ) {
                                var data = response.d;
                                // alert(typeof (data)); //it comes out to be string
                                //we need to parse it to JSON

                                data = $.parseJSON( data );

                                if ( data.Status == "000" ) {
                                    bootbox.alert( {
                                        message: data.Message,
                                        callback: function () {
                                            window.location.href = 'ApprovalEntries.aspx?status=Pending&parent=StaffClaims';
                                        }
                                    } )
                                } else {
                                    //error dialog
                                    bootbox.error( { title: "Authorization error", message: data.Message } );
                                }
                            }
                        },
                        error: function () {
                            bootbox.error( { title: "System error", message: "An error occured." } );
                        }
                    } );
                }
            }
        } );
    } );
    jQuery_3_3_1( '#Reject' ).on( 'click', function ( event ) {
        event.preventDefault();

        const queryString = window.location.search;
        const urlParams = new URLSearchParams( queryString );
        const ApprovalEntryNo = urlParams.get( 'ApprovalEntryNo' )

        bootbox.confirm( {
            title: "<i class='fa fa-times'></i> Reject application?",
            message: "Do you wish to reject this application?",
            buttons: {
                confirm: {
                    label: 'Yes',
                    className: 'btn-success'
                },
                cancel: {
                    label: 'No',
                    className: 'btn-danger'
                }
            },
            callback: function ( result ) {

                if ( result == true ) {

                    //prompt for comment

                    jQuery_3_3_1( '#ApplicationRejectionComment' ).modal( 'show' );

                    $( "#ApplicationRejectionComment #DocumentNo" ).val( ApprovalEntryNo );
                }
            }
        } );
    } );
    jQuery_3_3_1( '#SubmitCommentReject' ).click( function ( e ) {

        e.preventDefault();
        //validate

        var form = $( "#RejectForm" )
        if ( form[ 0 ].checkValidity() === false ) {
            event.preventDefault()
            event.stopPropagation()
            form.addClass( 'was-validated' );
        } else if ( form[ 0 ].checkValidity() === true ) {
            //showToast('Rejecting application');

            jQuery.ajax( {
                url: 'ViewTransportRequest.aspx/RejectApprovalRequest',
                type: "POST",
                data: '{param1:"' + $( '#DocumentNo' ).val() + '", param2:"' + $( '#Comment' ).val() + '"}',
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function ( response ) {

                    if ( response != null && response.d != null ) {
                        var data = response.d;
                        // alert(typeof (data)); //it comes out to be string
                        //we need to parse it to JSON
                        data = $.parseJSON( data );

                        //hideLoading();

                        bootbox.alert( {
                            message: data.Message,
                            callback: function () {
                                window.location.href = 'ApprovalEntries.aspx?status=Pending&parent=StaffClaims';
                            }
                        } )
                    }
                },
                error: function () {
                    bootbox.error( { title: "System error", message: "An error occured." } );
                }
            } );
        }
    } );

</script>

