@model LMS.Models.ViewTransportRequest

@{
    ViewBag.Title = "ViewTransportRequest";
}


<main id="main" class="main">
    <div id="wrapper">

        <!-- Sidebar -->
        <!-- #Include virtual="includes/Sidebar.aspx" -->


        <div id="content-wrapper">

            <div class="container-fluid">

                <!-- Breadcrumbs-->
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <a href="#">Dashboard</a>
                    </li>
                    <li class="breadcrumb-item">Transport Requests</li>
                    <li class="breadcrumb-item active">View Transport Request</li>
                </ol>
                <!-- DataTables Example -->
                <div class="card mb-3">
                    <div class="card-header">
                        <i class="fas fa-share-square"></i>
                        View Transport Request
                    </div>
                    <fieldset 
                       @if (Request["status"] == "Open")
                       {
                           Response.Write("enabled");
                       }
                       else
                       {
                           Response.Write("disabled");
                       } >
              <form runat ="server" novalidate="" action="#" method="POST" id="TransportRequestForm" autocomplete="off">
            <div class="card-body">
              <div class="row mb-5">
                <div class="col-md-4 mb-4">
                    <div>Transport Request Information</div>
                    <div class="text-muted small">Shows information for transport request</div>
                </div>
                  <div class="col-md-8">

                      <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                        <asp:label id="DimCode6Label" runat="server" />
                                    <asp:DropDownList CssClass="form-control" ID ="DimCode6" runat="server" AutoPostBack="false" required ="true" style="width:100%"> </asp:DropDownList>

                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The department must be selected</div>
                                </div>
                            </div>
                        </div>

                      <div class="row">
                       <div class="col-md-6">
                            <div class="form-group">
                                    <label class="form-control-label" for ="StartDateTime"> Start Date & Time when vehicle is required</label>
                                    <asp:TextBox class="form-control" id="StartDateTime" runat ="server" required ="true"></asp:TextBox>
                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The Start Date & Time when vehicle is required must be selected</div>
                              </div>
                         </div>

                         <div class="col-md-6">
                                <div class="form-group">
                                    <label class="form-control-label" for ="EndDateTime">End Date & Time when vehicle is required</label>
                                    <asp:TextBox class="form-control" id="EndDateTime" runat ="server" required ="true"></asp:TextBox>
                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The End Date & Time when vehicle is required must be selected</div>
                                </div>
                            </div>
                        </div>

                      <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label class="form-control-label">Purpose for which the vehicle is requested</label>
                                     <asp:TextBox TextMode="multiline" Columns="50" maxlength="150" Rows="3" runat="server" ID="Purpose" class="form-control MultiLineLimit" required ="true"></asp:TextBox>
                                     <p class="pull-right text-muted small" id="count_message"></p>
                                     <div class="valid-feedback">Looks good!</div>
                                     <div class="invalid-feedback">The purpose for which the vehicle is requested must be provided</div>
                                </div>
                            </div>
                        </div>
                      <div class="row">
                          <div class="col-md-6">
                            <div class="form-group">
                                    <asp:label id="DimCode1Label" runat="server" />
                                    <asp:DropDownList CssClass="form-control" ID ="DimCode1" runat="server" AutoPostBack="false" required ="true" style="width:100%"> </asp:DropDownList>
                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The Dimension Code 1 must be selected</div>
                              </div>
                         </div>

                           <div class="col-md-6">
                                <div class="form-group">
                                     <asp:label id="DimCode2Label" runat="server" />
                                    <asp:DropDownList CssClass="form-control" ID ="DimCode2" runat="server" AutoPostBack="false" required ="true" style="width:100%"> </asp:DropDownList>

                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The Dimension Code 2 must be selected</div>
                                </div>
                            </div>
                        </div>
                      <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                        <asp:label id="DimCode3Label" runat="server" />
                                    <asp:DropDownList CssClass="form-control" ID ="DimCode3" runat="server" AutoPostBack="false" required ="true" style="width:100%"> </asp:DropDownList>

                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The Dimension Code 3 must be selected</div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <asp:label id="DimCode4Label" runat="server" />
                                    <asp:DropDownList CssClass="form-control" ID ="DimCode4" runat="server" AutoPostBack="false" required ="true" style="width:100%"> </asp:DropDownList>

                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The Dimension Code 4 must be selected</div>
                                </div>
                            </div>
                        </div>
                      <div class="row">
                          <div class="col-md-12">
                            <div class="form-group">
                                    <label class="form-control-label" for ="Destination">Destination</label>
                                    <asp:TextBox CssClass="form-control" ID ="Destination" runat="server"  required ="true" > </asp:TextBox>
                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The destination of the trip must be provided</div>
                              </div>
                         </div>
                       </div>

                      <div class="row">
                          <div class="col-md-12">
                            <div class="form-group">
                                <label class="form-control-label" for ="VehicleClass">Class of vehicle</label>
                                <asp:DropDownList CssClass="form-control" ID ="VehicleClass" runat="server" AutoPostBack="false" required ="true" style="width:100%"> </asp:DropDownList>

                                <div class="valid-feedback">Looks good!</div>
                                <div class="invalid-feedback">The Vehicle class must be selected</div>
                            </div>
                         </div>
                      </div>

                      <div class="row">
                          <div class="col-md-6">
                            <div class="form-group">
                                    <label class="form-control-label" for ="EstimatedDistance">Estimated Distance of trip in Kms</label>
                                    <asp:TextBox CssClass="form-control" ID ="EstimatedDistance" runat="server"  required ="true" > </asp:TextBox>
                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The estimated distance of the trip must be provided</div>
                              </div>
                         </div>
                          <div class="col-md-6">
                            <div class="form-group">
                                    <label class="form-control-label" for ="EstimatedCost">Estimated cost of trip</label>
                                    <asp:TextBox CssClass="form-control" ID ="EstimatedCost" runat="server"  required ="true" > </asp:TextBox>
                                    <div class="valid-feedback">Looks good!</div>
                                    <div class="invalid-feedback">The estimated distance of the trip must be provided</div>
                              </div>
                         </div>
                        </div>

                    </div>
                </div>
             </div>
            </fieldset>

                    <div class="card-footer bg-light text-right">
                        @if (Request["status"] == "Open")
                        {
                        <button id="SubmitTransportRequest" class="btn btn-primary">Submit</button>
                        }
                        else if (Request["status"] == "Pending" && (! String.IsNullOrEmpty(Request["parent"]) && Request["parent"]=="Approver")) //&& sent from Approval Entries)
                        {
                        <button id="Approve" class="btn btn-success">Approve</button>
                        <button id="Reject" class="btn btn-danger">Reject</button>
                        <button id="ExitPage" class="btn btn-primary">Close</button>
                        }
                        else
                        {

                        <button id="Print" class="btn btn-primary">Print</button>
                        }
                    </div>
                    </form>

                    <div class="card-footer small text-muted">Updated today at  @DateTime.Now.ToShortTimeString() </div>
                </div>

            </div>
            <!-- /.container-fluid  -->
            <!-- Sticky Footer -->
            <footer class="sticky-footer">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright © @DateTime.Now.Year.ToString()  Brightsoft Technologies Limited</span>
                    </div>
                </div>
            </footer>

        </div>
        <!-- /.content-wrapper -->

    </div>

    <!--rejection comment modal-->
    <div class="modal fade" id="ApplicationRejectionComment" tabindex="-1" role="dialog" aria-labelledby="RejectionCommentTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="RejectionCommentTitle">Reject approval request</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <form novalidate="" action="#" method="POST" id="RejectForm" autocomplete="off">

                    <div class="modal-body">
                        <input class="form-control" id="DocumentNo" disabled required="required" hidden>
                        <div class="form-group">
                            <label for="Comment">Rejection Comment</label>
                            <textarea class="form-control MultiLineLimit" id="Comment" rows="3" required="required" data-limit=250></textarea>
                            <p class="pull-right text-muted small" id="count_message2"></p>
                            <div class="valid-feedback">Looks good!</div>
                            <div class="invalid-feedback">A rejection comment must be provided</div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        <button type="button" id="SubmitCommentReject" class="btn btn-primary">Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</main>

