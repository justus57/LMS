<script src="js/select2.min.js"></script>
<script>
    $( document ).ready( function () {

        const queryString = window.location.search;
        const urlParams = new URLSearchParams( queryString );
        const status = urlParams.get( 'status' )

        if ( status != "Open" ) {
            $( DimCode1 ).prop( "disabled", true );
            $( DimCode2 ).prop( "disabled", true );
            $( DimCode3 ).prop( "disabled", true );
            $( DimCode4 ).prop( "disabled", true );
            $( DimCode6 ).prop( "disabled", true );
            $( VehicleClass ).prop( "disabled", true );
        }


        $( '#DimCode1' ).select2( {
            sorter: function ( data ) {
                return data.sort( function ( a, b ) {
                    return a.text < b.text ? -1 : a.text > b.text ? 1 : 0;
                } );
            }
        } );
        $( '#DimCode2' ).select2( {
            sorter: function ( data ) {
                return data.sort( function ( a, b ) {
                    return a.text < b.text ? -1 : a.text > b.text ? 1 : 0;
                } );
            }
        } );
        $( '#DimCode3' ).select2( {
            sorter: function ( data ) {
                return data.sort( function ( a, b ) {
                    return a.text < b.text ? -1 : a.text > b.text ? 1 : 0;
                } );
            }
        } );
        $( '#DimCode4' ).select2( {
            sorter: function ( data ) {
                return data.sort( function ( a, b ) {
                    return a.text < b.text ? -1 : a.text > b.text ? 1 : 0;
                } );
            }
        } );
        $( '#DimCode6' ).select2( {
            sorter: function ( data ) {
                return data.sort( function ( a, b ) {
                    return a.text < b.text ? -1 : a.text > b.text ? 1 : 0;
                } );
            }
        } );
        $( '#VehicleClass' ).select2( {
            sorter: function ( data ) {
                return data.sort( function ( a, b ) {
                    return a.text < b.text ? -1 : a.text > b.text ? 1 : 0;
                } );
            }
        } );

        //datepickers
        $( "#StartDateTime" ).datetimepicker( {
            dateFormat: 'mm/dd/yy',

            onSelect: function ( date ) {

                var selectedDate = new Date( date );
                //var msecsInADay = 86400000;
                var endTime = selectedDate.getHours() + ":" + selectedDate.getMinutes();
                $( "#EndDateTime" ).datepicker( "option", "minTime", endTime );

                //Set Minimum Date of EndDatePicker After Selected Date of StartDatePicker
                $( "#EndDateTime" ).datepicker( "option", "minDate", date );
                $( "#EndDateTime" ).datepicker( "option", "maxDate", '+2y' );

            }

        } ).val();

        $( "#EndDateTime" ).datetimepicker( { dateFormat: 'mm/dd/yy' } ).val();

        //Load Transport req

        $.ajax( {
            url: 'ViewTransportRequest.aspx/LoadTransportRequest',
            type: "POST",
            dataType: "json",
            contentType: "application/json; charset=utf-8",
            success: function ( response ) {

                if ( response != null && response.d != null ) {
                    var data = response.d;

                    data = $.parseJSON( data );

                    $( '#StartDateTime' ).val( data.RequestStartDateTime );
                    $( '#EndDateTime' ).val( data.RequestEndDateTime );

                    $( "#DimCode1" ).val( data.GlobalDimension1Code ).change();
                    $( "#DimCode2" ).val( data.GlobalDimension2Code ).change();
                    $( "#DimCode3" ).val( data.ShortcutDimension3Code ).change();
                    $( "#DimCode4" ).val( data.ShortcutDimension4Code ).change();
                    $( "#DimCode6" ).val( data.ShortcutDimension6Code ).change();
                    $( "#EstimatedCost" ).val( data.EstimatedTripCost );
                    $( "#Balance" ).val( data.Balance );
                    $( "#VehicleClass" ).val( data.VehicleClassRequested ).change();
                    $( "#EstimatedDistance" ).val( data.EstimatedDistance );
                    $( "#Purpose" ).val( data.PurposeOfTrip );
                    $( "#Destination" ).val( data.Destination );


                }
            }
        } );
        $( "#VehicleClass" ).change( function () {


            $( '#EstimatedDistance' ).val( '' );

            if ( $( "#VehicleClass" ).val() != '' ) {

                //selection fields
                jQuery_3_3_1.ajax( {
                    url: 'ViewTransportRequest.aspx/GetVehicleClassRate',
                    type: "POST",
                    data: '{Code:"' + $( "#VehicleClass" ).val() + '" }',
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function ( response ) {

                        if ( response != null && response.d != null ) {
                            var data = response.d;
                            // alert(typeof (data)); //it comes out to be string
                            //we need to parse it to JSON
                            data = $.parseJSON( data );
                            $( '#Rate' ).val( data.Rate );
                        }
                    },
                    error: function () {
                        bootbox.error( { title: "System error", message: "An error occured." } );
                    }
                } );
            }
        } );

    } );
</script>


